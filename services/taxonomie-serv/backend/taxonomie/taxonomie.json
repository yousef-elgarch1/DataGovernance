  "categories": [
    {
      "class": "IDENTITE_PERSONNELLE",
      "class_en": "PERSONAL_IDENTITY",
      "type": "PII",
      "subclasses": [
        {
          "name": "Nom complet",
          "name_en": "Full name",
          "synonyms_fr": ["nom et prénom", "identité civile", "nom patronymique"],
          "synonyms_en": ["full legal name", "civil identity", "personal name"],
          "acronyms_fr": ["NOM"],
          "acronyms_en": ["FN"],
          "regex_patterns": [
            "\\b[A-Z][a-zàâçéèêëïîôùûü-]+\\s+[A-Z][a-zàâçéèêëïîôùûü-]+\\b",
            "\\b[A-Z][A-Z\\s]+\\b"
          ],
          "sensitivity_level": "high"
        },
        {
          "name": "CIN - Carte d'Identité Nationale",
          "name_en": "National Identity Card",
          "synonyms_fr": ["carte nationale", "pièce d'identité", "CNI"],
          "synonyms_en": ["ID card", "national ID"],
          "acronyms_fr": ["CIN", "CNI"],
          "acronyms_en": ["NIC"],
          "regex_patterns": [
            "\\b[A-Z]{1,2}\\d{5,8}\\b",
            "\\bCIN[:\\s]*[A-Z]{1,2}\\d{5,8}\\b"
          ],
          "format": "1-2 lettres + 5-8 chiffres",
          "sensitivity_level": "critical"
        },
        {
          "name": "Numéro de passeport",
          "name_en": "Passport number",
          "synonyms_fr": ["passeport", "document de voyage"],
          "synonyms_en": ["passport ID", "travel document"],
          "acronyms_fr": ["PP"],
          "acronyms_en": ["PP"],
          "regex_patterns": ["\\b[A-Z]{1,2}\\d{6,9}\\b", "\\b\\d{8,9}\\b"],
          "sensitivity_level": "critical"
        },
        {
          "name": "Date de naissance",
          "name_en": "Date of birth",
          "synonyms_fr": ["né(e) le", "date naissance", "DDN"],
          "synonyms_en": ["DOB", "birthdate"],
          "acronyms_fr": ["DDN"],
          "acronyms_en": ["DOB"],
          "regex_patterns": [
            "\\b(0[1-9]|[12][0-9]|3[01])[-/](0[1-9]|1[0-2])[-/](19|20)\\d{2}\\b",
            "\\b(19|20)\\d{2}[-/](0[1-9]|1[0-2])[-/](0[1-9]|[12][0-9]|3[01])\\b"
          ],
          "sensitivity_level": "high"
        },
        {
          "name": "Lieu de naissance",
          "name_en": "Place of birth",
          "synonyms_fr": ["né(e) à", "ville de naissance"],
          "synonyms_en": ["birthplace"],
          "acronyms_fr": ["LN"],
          "acronyms_en": ["POB"],
          "regex_patterns": ["\\bné(e)?\\s+(à|au|aux)\\s+[A-Z][a-zàâçéèêëïîôùûü-]+\\b"],
          "sensitivity_level": "medium"
        },
        {
          "name": "Nationalité",
          "name_en": "Nationality",
          "synonyms_fr": ["citoyenneté", "origine nationale"],
          "synonyms_en": ["citizenship"],
          "regex_patterns": ["\\b(marocain|marocaine|français|française|algérien|algérienne|tunisien|tunisienne|espagnol|espagnole)\\b"],
          "sensitivity_level": "high"
        }
      ]
    },

